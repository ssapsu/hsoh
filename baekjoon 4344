#include <stdio.h>

int main()
{
	int get_num,n=0,how_many;
	int score[1000];
	double sum = 0, score_avg,above_avg;
	scanf("%d", &get_num);
	while (n < get_num)
	{
		scanf("%d", &how_many);
		for (int i = 0; i < how_many; i++)
		{
			scanf("%d", (score+i));
			sum += score[i];
		}
		score_avg = sum / how_many;
		sum = 0;
		for(int i = 0; i < how_many; i++)
		{
			if (score[i] > score_avg)
				sum += 1;
		}
		above_avg = sum / how_many*100;
		printf("%.3lf%%\n", above_avg);
		sum = 0;
		above_avg = 0;
		n++;
	}
	return 0;
}
